  <appSettings>
    <!--<add key="ConnString" value="Data Source=(LocalDB)\v11.0;AttachDbFilename=C:\svn\CMS\trunk\cms\App_Data\CMS.mdf;Integrated Security=True"/>-->

    <!--<add key="ConnString" value="Data Source=192.168.1.188\SQL2008;Initial Catalog=trip;User ID=sa;Password=eee-333;" />-->
  </appSettings>
  <connectionStrings>
    <!--<add name="ConnString" providerName="System.Data.SqlClient" connectionString="Data Source=(LocalDb)\v11.0;Initial Catalog=SportsStore; &#xD;&#xA;         Integrated Security=SSPI;AttachDBFilename=|DataDirectory|\CMS.mdf" />-->
    <!--<add name="ConnString" connectionString="Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|\CMS.mdf;Initial Catalog=SportsStore;Integrated Security=True;User Instance=True" providerName="System.Data.SqlClient" />-->
    <!--<add name="ConnString" providerName="System.Data.SqlClient" connectionString="Data Source=(LocalDb)\v11.0;Initial Catalog=SportsStore;&#xD;&#xA; &#xD;&#xA;         Integrated Security=SSPI;AttachDBFilename=|DataDirectory|\CMS.mdf" />-->
